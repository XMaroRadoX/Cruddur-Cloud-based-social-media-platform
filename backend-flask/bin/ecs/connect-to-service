#! /usr/bin/bash
if [ -z "$1" ]
then
echo "No TASK_ID name supplied eg ./bin/ecs/connect-to-service task-id"
exit 1
fi
TASK_ID=$1

if [ -z "$2" ]
then
echo "No Container name provided supplied eg ./bin/ecs/connect-to-service tas"
exit 1
fi
CONTAINER_NAME=$2

  aws ecs execute-command  \
--region $AWS_DEFAULT_REGION \
--cluster cruddur \
--task $TASK_ID \
--container $CONTAINER_NAME \
--command "/bin/bash" \
--interactive